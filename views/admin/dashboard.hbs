  {{#if msg}}
  <div class="alert alert-warning alert-dismissible fade show" role="alert">
    <strong>{{msg}}</strong>
    <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
  </div>
  <script>
    window.setTimeout(()=>{
      window.location="/admin"
    },2000)
  </script>
  {{/if}}
{{#if users}}
<table class="table table-striped">

  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">name</th>
      <th scope="col">email</th>
      <th scope="col"></th>
      <th scope="col"></th>
    </tr>
  </thead>
  <tbody>
      {{#each users}}
        <tr>
          <th scope="row"></th>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
          <td><a href="admin/edit?msg={{this._id}}" class="btn">Edit</a></td>
          <td><a href="admin/delete/{{this._id}}" class="btn">Delete</a></td>
        </tr>
      {{/each}}
  </tbody>
</table>
    {{else}}

      <p class="text-center">No user found !</p>
    {{/if}}